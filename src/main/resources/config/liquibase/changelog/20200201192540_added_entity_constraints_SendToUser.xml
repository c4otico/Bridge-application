<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity SendToUser.
    -->
    <changeSet id="20200201192540-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="formula_id"
                                 baseTableName="send_to_user"
                                 constraintName="fk_send_to_user_formula_id"
                                 referencedColumnNames="id"
                                 referencedTableName="formula"/>

        <addForeignKeyConstraint baseColumnNames="developer_id"
                                 baseTableName="send_to_user"
                                 constraintName="fk_send_to_user_developer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="developer"/>

    </changeSet>
</databaseChangeLog>
